{
  "name": "@mintlify/validation",
  "version": "0.1.555",
  "description": "Validates mint.json files",
  "author": "Mintlify, Inc.",
  "bugs": {
    "url": "https://github.com/mintlify/docs/issues"
  },
  "license": "Elastic-2.0",
  "keywords": [
    "mintlify",
    "mint",
    "validation"
  ],
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc --project tsconfig.build.json",
    "clean:build": "rimraf dist",
    "clean:all": "rimraf node_modules .eslintcache && yarn clean:build",
    "test": "vitest run",
    "type": "tsc --noEmit",
    "lint": "eslint . --cache",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "dependencies": {
    "@mintlify/mdx": "^3.0.4",
    "@mintlify/models": "0.0.255",
    "arktype": "2.1.27",
    "js-yaml": "4.1.0",
    "lcm": "0.0.3",
    "lodash": "4.17.21",
    "object-hash": "3.0.0",
    "openapi-types": "12.1.3",
    "uuid": "11.1.0",
    "zod": "3.21.4",
    "zod-to-json-schema": "3.20.4"
  },
  "devDependencies": {
    "@mintlify/eslint-config-typescript": "1.0.13",
    "@mintlify/prettier-config": "1.0.4",
    "@mintlify/ts-config": "2.0.2",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@tsconfig/recommended": "1.0.2",
    "@types/lcm": "0.0.0",
    "@types/object-hash": "3.0.6",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "eslint": "8.36.0",
    "openapi-types": "12.1.3",
    "prettier": "3.1.1",
    "rimraf": "5.0.1",
    "ts-expect": "1.3.0",
    "typescript": "5.5.3",
    "vitest": "2.0.4"
  },
  "gitHead": "04e311ad6ebfc64daff8e7e4529dd12114c9e9a9"
}
