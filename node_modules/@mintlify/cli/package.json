{
  "name": "@mintlify/cli",
  "version": "4.0.952",
  "description": "The Mintlify CLI",
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Mintlify, Inc.",
  "bugs": {
    "url": "https://github.com/mintlify/docs/issues"
  },
  "license": "Elastic-2.0",
  "keywords": [
    "mintlify",
    "mint",
    "cli"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mintlify/mint",
    "directory": "packages/cli"
  },
  "exports": "./bin/index.js",
  "bin": {
    "mint": "bin/index.js",
    "mintlify": "bin/index.js"
  },
  "scripts": {
    "dev": "yarn build && NODE_NO_WARNINGS=1 node bin/index.js",
    "prepare": "npm run build",
    "build": "tsc --project tsconfig.build.json",
    "clean:build": "rimraf bin",
    "clean:all": "rimraf node_modules .eslintcache && yarn clean:build",
    "watch": "tsc --watch",
    "lint": "eslint src --cache",
    "test": "vitest run",
    "type": "tsc --noEmit",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "dependencies": {
    "@inquirer/prompts": "7.9.0",
    "@mintlify/common": "1.0.727",
    "@mintlify/link-rot": "3.0.887",
    "@mintlify/models": "0.0.271",
    "@mintlify/prebuild": "1.0.863",
    "@mintlify/previewing": "4.0.920",
    "@mintlify/validation": "0.1.593",
    "adm-zip": "0.5.16",
    "chalk": "5.2.0",
    "color": "4.2.3",
    "detect-port": "1.5.1",
    "front-matter": "4.0.2",
    "fs-extra": "11.2.0",
    "ink": "6.3.0",
    "inquirer": "12.3.0",
    "js-yaml": "4.1.0",
    "mdast-util-mdx-jsx": "3.2.0",
    "react": "19.2.3",
    "semver": "7.7.2",
    "unist-util-visit": "5.0.0",
    "yargs": "17.7.1"
  },
  "devDependencies": {
    "@mintlify/eslint-config-typescript": "1.0.13",
    "@mintlify/prettier-config": "1.0.4",
    "@mintlify/ts-config": "2.0.2",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@tsconfig/recommended": "1.0.2",
    "@types/adm-zip": "0.5.7",
    "@types/detect-port": "1.3.2",
    "@types/mdast": "4.0.4",
    "@types/node": "18.15.0",
    "@types/yargs": "17.0.22",
    "@typescript-eslint/parser": "6.9.1",
    "eslint": "8.36.0",
    "openapi-types": "12.1.3",
    "prettier": "3.1.1",
    "rimraf": "5.0.1",
    "typescript": "5.5.3",
    "vitest": "2.0.4",
    "vitest-mock-process": "1.0.4"
  },
  "gitHead": "fa2f55e46fe8db4662e3420ca88e16468c43781a"
}
